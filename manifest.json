{
    "manifest_version": 3,
    "name": "Just Enough Reading",
    "version": "1.0",
    "description": "A Chrome extension for enhancing reading experience.",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
    "128": "assets/icon-128.png",
    "256": "assets/icon-256.png",
    "512": "assets/icon-512.png"
    }
}